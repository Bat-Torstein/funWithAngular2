{
  "name": "funWithAngular2",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "karma start karma.conf.js",
    "remap-istanbul": "remap-istanbul -i ./report/coverage/coverage-final.json -o ./report/coverage/html-report -t html",
    "testcover": "karma start karma.cover.conf.js && npm run remap-istanbul",
    "lint": "node_modules/.bin/tslint -c tslint.json **/*.ts -e node_modules/**/*.ts",
    "copy": "node_modules/.bin/grunt copy",
    "build": "npm run tsc && npm run copy && npm run pack",
    "pack": "node_modules/.bin/webpack --progress"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.4.1",
    "@angular/compiler": "~2.4.1",
    "@angular/core": "~2.4.1",
    "@angular/forms": "~2.4.1",
    "@angular/http": "~2.4.1",
    "@angular/platform-browser": "~2.4.1",
    "@angular/platform-browser-dynamic": "~2.4.1",
    "@angular/router": "~3.4.1",
    "@angular/upgrade": "~2.4.1",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "primeng": "^1.0.0-beta.20",
    "rxjs": "~5.0.2",
    "systemjs": "0.19.39",
    "underscore": "^1.8.3",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "babel-minify-webpack-plugin": "^0.2.0",
    "codelyzer": "^4.0.2",
    "concurrently": "^2.0.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^1.4.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "html-loader": "^0.4.4",
    "jasmine-core": "^2.5.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "less": "^2.7.1",
    "lite-server": "^2.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "reflect-metadata": "^0.1.8",
    "remap-istanbul": "^0.6.4",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  }
}
