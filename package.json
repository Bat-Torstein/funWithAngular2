{
  "name": "funWithAngular2",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "karma start karma.conf.js",
    "remap-istanbul": "remap-istanbul -i ./report/coverage/coverage-final.json -o ./report/coverage/html-report -t html",
    "testcover": "karma start karma.cover.conf.js && npm run remap-istanbul",
    "lint": "node_modules/.bin/tslint -c tslint.json **/*.ts -e node_modules/**/*.ts"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "@angular/upgrade": "~2.1.0",
    "@angular2-material/core": "^2.0.0-alpha.8-2",
    "@angular2-material/menu": "^2.0.0-alpha.8-2",
    "@angular2-material/progress-circle": "^2.0.0-alpha.8-2",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "jasmine-core": "^2.5.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "ng2-modal": "0.0.20",
    "ng2-toastr": "^1.1.0",
    "remap-istanbul": "^0.6.4",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "underscore": "^1.8.3",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "codelyzer": "^1.0.0-beta.0",
    "concurrently": "^2.0.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^1.4.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "karma-phantomjs2-launcher": "^0.5.0",
    "less": "^2.7.1",
    "lite-server": "^2.2.0",
    "reflect-metadata": "^0.1.8",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.3.2"
  }
